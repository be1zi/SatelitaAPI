type Mutation {
    createUser(auth: AuthData) : User
    signinUser(auth: AuthData) : SigninPayload
}

type Query {
    getNumber: Int
}

type User {
    id: ID
    login: String
    password: String
}

input AuthData {
    login: String
    password: String
}

type SigninPayload {
    token: String
    expiryTime: Int
}