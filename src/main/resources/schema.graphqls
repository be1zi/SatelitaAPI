type Query {
    bookById(id: ID): Book
}

type Book {
    id: ID
    name: String
    pageCount: Int
    author: Author
}

type Author {
    id: ID
    firstName: String
    lastName: String
}





type Mutation {
    createUser(authProvider: AuthData): User
}

type User {
    id: ID
    login: String
    password: String
}

input AuthData {
    login: String
    password: String
}